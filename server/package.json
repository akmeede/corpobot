{
  "name": "backend",
  "version": "0.0.0",
  "scripts": {
    "start":
      "nodemon -e js,json,graphql,gql -x node -r dotenv/config src/index.js",
    "debug":
      "nodemon -e js,json,graphql,gql -x node --inspect -r dotenv/config src/index.js",
    "playground": "graphql playground",
    "dev": "npm-run-all --parallel start playground",
    "prisma": "prisma"
  },
  "dependencies": {
    "bcryptjs": "2.4.3",
    "graphql-yoga": "1.8.5",
    "jsonwebtoken": "8.2.1",
    "prisma-binding": "1.5.17"
  },
  "devDependencies": {
    "dotenv": "5.0.1",
    "graphql-cli": "2.15.9",
    "nodemon": "1.17.3",
    "npm-run-all": "^4.1.2",
    "prisma": "1.6.0"
  },
  "engines": {
    "node": ">=9.0.0",
    "yarn": ">=1.5.0"
  }
}
