{
  "name": "backend",
  "version": "0.0.0",
  "scripts": {
    "start": "nodemon -e js,json,graphql,gql -x node -r dotenv/config src/index.js",
    "debug": "nodemon -e js,json,graphql,gql -x node --inspect -r dotenv/config src/index.js",
    "playground": "graphql --dotenv .env playground",
    "dev": "npm-run-all --parallel start playground",
    "prisma": "prisma"
  },
  "dependencies": {
    "graphql-yoga": "^1.14.7",
    "jsonwebtoken": "^8.2.2",
    "prisma-binding": "^2.0.2"
  },
  "devDependencies": {
    "dotenv": "^6.0.0",
    "express-jwt": "^5.3.1",
    "graphql-cli": "^2.16.2",
    "jwks-rsa": "^1.2.1",
    "nodemon": "1.17.5",
    "npm-run-all": "^4.1.3",
    "prisma": "^1.9.0"
  },
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.0.0"
  }
}
